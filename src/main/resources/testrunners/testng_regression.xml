<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Opencart App Test Regression Suite" thread-count="4"
	parallel="tests" verbose="4">
	<listeners>
		<listener
			class-name="com.testing.ecom.cart.listeners.ExtentReportListener" />
			<listener
			class-name="com.testing.ecom.cart.listeners.TestAllureListener" />
	</listeners>
	<test name="Login Page Test">
		<classes>
			<class name="com.testing.ecom.cart.test.LoginPageTest"></class>
		</classes>
	</test>
	<test name="Accounts Page Test">
		<classes>
			<class name="com.testing.ecom.cart.test.AccountsPageTest"></class>
		</classes>
	</test>
	<!-- <test name="Product Info Page Test">
		<classes>
			<class name="com.testing.ecom.cart.test.ProductInfoPageTest"></class>
		</classes>
	</test>
	<test name="Register Page Test">
		<classes>
			<class name="com.testing.ecom.cart.test.RegisterPageTest"></class>
		</classes>
	</test>-->
</suite>